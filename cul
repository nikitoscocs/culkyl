import java.util.Scanner;

public class Cul {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        String input = scanner.nextLine();
        scanner.close();

        String[] parts = input.split(" ");

        // Проверка, что ровно три элемента: число, оператор, число
        if (parts.length != 3) {
            System.out.println("Ошибка: У вас больше 3 операторов");
            return;
        }

        try {
            int a = Integer.parseInt(parts[0]);
            String operator = parts[1];
            int b = Integer.parseInt(parts[2]);

            // Проверка, что числа от 1 до 10
            if (a < 1 || a > 10 || b < 1 || b > 10) {
                System.out.println("Ошибка: Числа должны быть от 1 до 10");
                return;
            }

            int result = 0;
            if (operator.equals("+")) {
                result = a + b;
            } else if (operator.equals("-")) {
                result = a - b;
            } else if (operator.equals("*")) {
                result = a * b;
            } else if (operator.equals("/")) {
                result = a / b; // целочисленное деление
            } else {
                System.out.println("Ошибка: Неизвестный оператор");
                return;
            }

            System.out.println(result);

        } catch (NumberFormatException e) {
            System.out.println("Ошибка: Число должно быть целым");
        } catch (ArithmeticException e) {
            System.out.println("throws Exception");
        }
    }
}
